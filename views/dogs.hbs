<h1>Dogs in adoption</h1>
<div>
  <ul>
    {{#each dogs}}
      <li class="singleDog"><a href="/dog/{{this.id}}">{{this.dogName}}<br
          />{{this.dogAge}}<br />{{#if this.dogPhotoURL}}<img
              src={{this.dogPhotoURL}}
              alt={{this.dogBreed}}
              width="200px"
              style="border-radius: 100%;"
            />{{/if}}</a></li>
    {{/each}}
  </ul>
</div>